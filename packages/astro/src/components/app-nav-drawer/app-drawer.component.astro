---

<dialog class="db-drawer">
  <article
    class="db-drawer-container db-header-drawer"
    data-spacing="small"
    data-rounded="true"
  >
    <header class="db-drawer-header">
      <div class="db-drawer-header-text"></div>
      <button
        class="db-button button-close-drawer"
        type="button"
        data-icon="cross"
        data-variant="ghost"
        data-no-text="true"
      >
        Close
      </button>
    </header>
    <div class="db-drawer-content">
      <div class="db-header-drawer-navigation">
        <div class="db-header-navigation">
          <nav id="OVERWRITE_DEFAULT_ID" class="db-navigation">
            <menu>
              <li class="db-navigation-item" data-icon="x_placeholder">
                <button
                  class="db-navigation-item-expand-button"
                  aria-haspopup="false"
                  aria-expanded="false"
                >
                  <a href="#">Link</a>
                </button>
                <menu
                  class="db-sub-navigation"
                  data-force-close="false"
                  id="sub-navigation-7357791899822511"></menu>
              </li>
              <li
                class="db-navigation-item"
                data-icon="x_placeholder"
                aria-disabled="true"
              >
                <button
                  class="db-navigation-item-expand-button"
                  aria-haspopup="false"
                  aria-expanded="false"
                  disabled=""
                >
                  <a href="#">Link disabled</a>
                </button>
                <menu
                  class="db-sub-navigation"
                  data-force-close="false"
                  id="sub-navigation-5586645993890211"></menu>
              </li>
            </menu>
          </nav>
        </div>
        <div class="db-header-meta-navigation">
          <a
            class="db-link"
            href="#"
            tabindex="0"
            data-show-icon-trailing="true"
            data-content="internal">Imprint</a
          >
        </div>
      </div>
      <div class="db-header-secondary-action">
        <button
          class="db-button"
          type="submit"
          data-icon="x_placeholder"
          data-variant="ghost"
          data-no-text="true"
        >
          Profile
        </button>
      </div>
    </div>
  </article>
</dialog>
